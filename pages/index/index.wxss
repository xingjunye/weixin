.swiper-box swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper-box swiper image {
  width: 100%;
}
.index-cate {
  display: flex;
}
.index-cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index-cate navigator image {
  width: 100%;
}
.index-floor .floor-grop {
  margin-bottom: 14rpx;
}
.index-floor .floor-grop .floor-title image {
  width: 100%;
}
.index-floor .floor-grop .floor-list {
  width: 100%;
  overflow: hidden;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.index-floor .floor-grop .floor-list navigator {
  float: left;
  width: 33.33%;
  box-sizing: border-box;
}
.index-floor .floor-grop .floor-list navigator:first-child {
  height: 55.45422414vw;
}
.index-floor .floor-grop .floor-list navigator:nth-last-child(-n + 4) {
  height: 27.72711207vw;
  padding-left: 10rpx;
}
.index-floor .floor-grop .floor-list navigator:nth-of-type(2) {
  padding-bottom: 5rpx;
}
.index-floor .floor-grop .floor-list navigator:nth-of-type(3) {
  padding-bottom: 5rpx;
}
.index-floor .floor-grop .floor-list navigator:nth-of-type(4) {
  padding-top: 5rpx;
}
.index-floor .floor-grop .floor-list navigator:nth-of-type(5) {
  padding-top: 5rpx;
}
.index-floor .floor-grop .floor-list navigator image {
  width: 100%;
  height: 100%;
}
